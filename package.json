{
  "name": "gcp-billing-dashboard",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "npm run version:info:gen && ng serve --host 0.0.0.0 --disableHostCheck --proxy-config proxy.conf.json",
    "build_prod": "npm run version:info:gen && ng build --prod --aot=false --build-optimizer=false --baseHref=/ --output-path=./dist/",
    "build_aot": "npm run version:info:gen && ng build --prod --baseHref=/ --output-path=./dist/",
    "build_aot_stats": "npm run version:info:gen && ng build --baseHref=/ --build-optimizer --aot --output-path=./dist/ --stats-json",
    "build_aot_gzip": "npm run version:info:gen && ng build --baseHref=/ --build-optimizer --aot --output-path=./dist/ && gzipper --verbose --gzip-level 9 ./dist/",
    "build_prod_aot_gzip": "npm run version:info:gen && ng build --baseHref=/ --prod --output-path=./dist/ && gzipper --verbose --gzip-level 9 ./dist",
    "prepare_nginx": "sudo cp -rfv ./nginx/default /etc/nginx/sites-enabled && sudo systemctl restart nginx",
    "prepare_ng_app": "npm run build_prod_aot_gzip && sudo cp -rfv ./dist/* /var/www/html",
    "deploy_aot_gzip": "npm run prepare_nginx && npm run prepare_ng_app",
    "analyze_es5": "webpack-bundle-analyzer ./dist/stats-es5.json",
    "analyze_es2015": "webpack-bundle-analyzer ./dist/stats-es2015.json",
    "test": "npm run version:info:gen && ng test",
    "test:CI": "npm run version:info:gen && ng test --browsers ChromeHeadless --no-watch --code-coverage",
    "e2e": "ng e2e",
    "lint:fix": "ng lint gcp-billing-dashboard --fix",
    "lint:check": "ng lint",
    "format": "prettier \"src/**/*.{css,html,js,json,md,scss,ts}\"",
    "format:fix": "prettier --write \"src/**/*.{css,html,js,json,md,scss,ts}\"",
    "format:check": "prettier -c \"src/**/*.{css,html,js,json,md,scss,ts}\"",
    "version:info:gen": "node version-info-gen.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.0",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.0",
    "@angular/compiler": "~8.2.0",
    "@angular/core": "~8.2.0",
    "@angular/forms": "~8.2.0",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.0",
    "@angular/platform-browser-dynamic": "~8.2.0",
    "@angular/router": "~8.2.0",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "git-describe": "^4.0.4",
    "hammerjs": "^2.0.8",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "rgb-hex": "^3.0.0",
    "rxjs": "~6.4.0",
    "ts-md5": "^1.2.7",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.802.0",
    "@angular/cli": "~8.2.0",
    "@angular/compiler-cli": "~8.2.0",
    "@angular/language-service": "~8.2.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^3.3.31",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "gzipper": "^3.4.2",
    "husky": "^3.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "prettier": "^1.19.1",
    "prettier-tslint": "^0.4.2",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.5.3",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
